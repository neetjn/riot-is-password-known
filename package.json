{
  "name": "@neetjn/riot-is-password-known",
  "version": "0.0.1",
  "description": "Riot.js mixin to check a given password against haveibeenpwned.com.",
  "main": "dist/riot-is-password-known.js",
  "scripts": {
    "lint": "npx eslint src/**.js",
    "pretty": "npx prettier-eslint --write src/**.js && npx prettier-eslint --write __tests__/**.js",
    "build": "npm run lint && npm run pretty && node_modules/.bin/parcel build src/plugin.js -o riot-is-password-known.js --global PasswordKnown",
    "test": "npm run lint && node_modules/.bin/jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neetjn/riot-is-password-known.git"
  },
  "keywords": [
    "riot",
    "mixin",
    "password",
    "security",
    "pawned"
  ],
  "author": "John Nolette",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neetjn/riot-is-password-known/issues"
  },
  "homepage": "https://github.com/neetjn/riot-is-password-known#readme",
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.0.0-beta.54",
    "@babel/register": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.4.0",
    "babel-loader": "^8.0.5",
    "eslint": "^5.10.1",
    "jest": "^24.1.0",
    "jest-dom": "^3.0.0",
    "js-sha1": "^0.6.0",
    "parcel": "^1.11.0",
    "parcel-bundler": "^1.11.0",
    "prettier": "^1.16.4",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "riot": "^3.13.2"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "verbose": true
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  }
}
